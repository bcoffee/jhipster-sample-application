<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity ReportSubscription.
    -->
    <changeSet id="20190104175746-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="subscription_report_id"
                                 baseTableName="report_subscription"
                                 constraintName="fk_report_subscription_subscription_report_id"
                                 referencedColumnNames="id"
                                 referencedTableName="report"/>

        <addForeignKeyConstraint baseColumnNames="subscription_distribution_list_id"
                                 baseTableName="report_subscription"
                                 constraintName="fk_report_subscription_subscription_distribution_list_id"
                                 referencedColumnNames="id"
                                 referencedTableName="distribution_list"/>

        <addForeignKeyConstraint baseColumnNames="subscription_user_id"
                                 baseTableName="report_subscription"
                                 constraintName="fk_report_subscription_subscription_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

    </changeSet>
</databaseChangeLog>
